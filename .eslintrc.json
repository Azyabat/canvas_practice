{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "plugin:prettier/recommended",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "prettier"
    ],
    "rules": {
        "consistent-return":"off",
        "no-param-reassign":"off",
        "symbol-description":"off",
        "prefer-destructuring":"off",
        "class-methods-use-this":"off",
        "no-console":"error"
    }
}
